<?xml version="1.0" encoding="UTF-8"?>
<bpmConfig>
	<uploadPath>d:/upload/</uploadPath>
	
	<export>
		<bean name="order">
			<!-- 导出EXCEL的列 -->
			<head-field>orderCode,contractNumber,mainType,type,sale,saleOrg,productGroup,saleGroup,accountGroup,contractType,totalExpense</head-field>
			<item-field>material,materialDesc,orderNumber_1,unit,price,factory,costCenter,profitCenter,wbs,refuseReason</item-field>
			<!-- 导出EXCEL的列对应的描述 -->
			<property name="contractNumber">合同号</property>
			<property name="mainType">主合同类型</property>
			<property name="contractType">合同类型</property>
			<property name="sale">售达方</property>
			<property name="saleOrg">销售组织</property>
			<property name="productGroup">产品组</property>
			<property name="saleGroup">销售组</property>
			<property name="accountGroup">账户组</property>
			<property name="orderCode">订单号</property>			
			<property name="type">订单类型</property>
			<property name="totalExpense">总金额</property>
			<!-- item -->
			<property name="material">物料号</property>
			<property name="materialDesc">物料描述</property>
			<property name="orderNumber_1">订单数量</property>
			<property name="unit">单位</property>
			<property name="price">单价</property>			
			<property name="factory">工厂</property>
			<property name="costCenter">成本中心</property>
			<property name="profitCenter">利润中心</property>
			<property name="wbs">WBS元素</property>
			<property name="refuseReason">拒绝原因</property>
		</bean>	
		<bean name="project">
			<head-field>wbs_id,subcontract_name01,subcontract_name02,entry_amount01,subcontract_amount01,rate01,entry_amount02,subcontract_amount02,rate02</head-field>			
			<item-field></item-field>
			<!-- 导出EXCEL的列对应的描述 -->
			<property name="wbs_id">WBS代码</property>
			<property name="subcontract_name01">客户名称</property>
			<property name="subcontract_name02">供应商名称</property>
			<property name="entry_amount01">收入总金额</property>
			<property name="subcontract_amount01">累计收款金额</property>
			<property name="rate01">收款比例</property>
			<property name="entry_amount02">发包总金额</property>
			<property name="subcontract_amount02">累计发包付款</property>
			<property name="rate02">累计付款比例</property>
		</bean>	
		<bean name="supplier">
			<head-field>subcontract_code,subcontract_name,entry_amount02,subcontract_amount02,rate02,entry_amount01,subcontract_amount01,rate01</head-field>			
			<item-field></item-field>
			<!-- 导出EXCEL的列对应的描述 -->
			<property name="subcontract_code">供应商代码</property>
			<property name="subcontract_name">客户名称</property>
			<property name="entry_amount02">收入总金额</property>
			<property name="subcontract_amount02">累计收款金额</property>
			<property name="rate02">收款比例</property>
			<property name="entry_amount01">发包总金额</property>
			<property name="subcontract_amount01">累计发包付款</property>
			<property name="rate01">累计付款比例</property>
		</bean>	
		<bean name="customer">
			<head-field>subcontract_code,wbs_id,entry_amount01,subcontract_amount01,rate01,entry_amount02,subcontract_amount02,rate02</head-field>			
			<item-field></item-field>
			<!-- 导出EXCEL的列对应的描述 -->
			<property name="subcontract_code">客户代码</property>
			<property name="wbs_id">WBS代码</property>
			<property name="entry_amount01">收入总金额</property>
			<property name="subcontract_amount01">累计收款金额</property>
			<property name="rate01">收款比例</property>
			<property name="entry_amount02">发包总金额</property>
			<property name="subcontract_amount02">累计发包付款</property>
			<property name="rate02">累计付款比例</property>
		</bean>	
		<bean name="supplierWBS">
			<head-field>wbs_id,subcontract_name01,subcontract_code02,entry_amount01,subcontract_amount01,rate01,entry_amount02,subcontract_amount02,rate02</head-field>			
			<item-field></item-field>
			<!-- 导出EXCEL的列对应的描述 -->
			<property name="wbs_id">WBS代码</property>
			<property name="subcontract_name01">客户名称</property>
			<property name="subcontract_code02">供应商代码</property>
			<property name="entry_amount01">收入总金额</property>
			<property name="subcontract_amount01">累计收款金额</property>
			<property name="rate01">收款比例</property>
			<property name="entry_amount02">发包总金额</property>
			<property name="subcontract_amount02">累计发包付款</property>
			<property name="rate02">累计付款比例</property>
		</bean>	
		<bean name="loanColl">
			<head-field>employee_name,employee_id,dept_name_cn,cost_center,start_date,description,currency,money,moneyRate,theday,status</head-field>			
			<item-field></item-field>
			<!-- 导出EXCEL的列对应的描述 -->
			<property name="employee_name">借款人</property>
			<property name="employee_id">借款人代码</property>
			<property name="dept_name_cn">成本中心</property>
			<property name="cost_center">成本中心代码</property>
			<property name="start_date">借款日期</property>
			<property name="description">借款用途</property>
			<property name="currency">货币单位</property>
			<property name="money">借款余额</property>
			<property name="moneyRate">本位币余额</property>
			<property name="theday">账龄</property>
			<property name="status">还款状态</property>
		</bean>	
	</export>
	<maps>
		<map key="contract" value="ContractJbpmManager"/>
		<map key="purchase" value="PurchaseManager"/>
		<map key="lowpurchase" value="PurchaseManager"/>
		<map key="cashPayout" value="CashPayoutJbpmManager"/>
		<map key="fee" value="FeeJbpmManager"/>
		<map key="projectApproval" value="ProjectApprovalJbpmManager"/>
	</maps>	
</bpmConfig>
